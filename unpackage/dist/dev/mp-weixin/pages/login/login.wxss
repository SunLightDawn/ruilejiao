.circle-container {
  position: relative;
  float: left;
  width: 42px;
  height: 42px;
}
.circle-item {
  position: absolute;
  left: 0;
  top: 0px;
  width: 42px;
  height: 42px;
  border-radius: 50%;
  background-color: #9dc0ce;
}
.circle-left-wrap,
.circle-right-wrap {
  position: absolute;
  left: 0;
  top: 0;
  width: 21px;
  height: 42px;
  overflow: hidden;
}
.circle-right-wrap {
  left: 21px;
}
.left,
.right {
  position: absolute;
  top: 0;
  left: 0;
  width: 42px;
  height: 42px;
  border-radius: 50%;
  background: #e4e4e4;
}
.left {
  clip: rect(0, 21px, auto, 0);
}
.right {
  clip: rect(0, auto, auto, 21px);
  left: -21px;
}
.mask {
  position: absolute;
  top: 2px;
  left: 2px;
  width: 38px;
  height: 38px;
  border-radius: 50%;
  background-color: #fff;
}
.jinduInfo {
